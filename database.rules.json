{
  "rules": {
    ".read": "false",
    ".write": "false",
    "polymon": {
      ".read": "auth != null",
      ".indexOn": "shortName"
    },
    "users": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid",
        "team": {
          ".indexOn": "id"
        },
        "caughtPolymon": {
          ".indexOn": "id"
        },
        "achievements": {
          ".write": "false",
          ".indexOn": "id"
        }
      }
    }
  }
}
